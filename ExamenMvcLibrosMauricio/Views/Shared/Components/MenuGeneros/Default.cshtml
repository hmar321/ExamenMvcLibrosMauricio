@model List<Genero>
<li class="nav-item dropdown">
    <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown">
        Generos
    </a>
    <ul class="dropdown-menu">
        @foreach (Genero genero in Model)
        {
            <li>
                <a class="dropdown-item" asp-controller="Libro" asp-action="Genero" asp-route-idgenero="@genero.IdGenero">@genero.Nombre</a>
            </li>
        }

    </ul>
</li>
